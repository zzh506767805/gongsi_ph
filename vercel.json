{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/phresearch/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/phresearch/$1"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "PRODUCTHUNT_DEVELOPER_TOKEN": "@producthunt_developer_token"
  }
}